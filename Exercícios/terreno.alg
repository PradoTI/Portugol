Algoritmo "retangulo"

Var


   base, altura, area, perimetro, diagonal : real

Inicio

  escreva("Base do retangulo: ")
  leia(base)
  escreva("Altura do retangulo: ")
  leia(altura)

  area <- base * altura
  perimetro <- (base + altura) * 2
  diagonal <- 1/2 ( - 8 * altura)
  
  escreval ("Area =", area)
  escreval ("Perimetro =", perimetro)
  escreval ("Diagonal =", diagonal)




Fimalgoritmo