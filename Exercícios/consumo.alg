Algoritmo "consumo"

Var

   distancia : inteiro
   combustivel : real
   consumoMedio : real


Inicio

     escreva("Distancia percorrida: ")
     leia(distancia)
     escreva("Combustivel gasto: ")
     leia(combustivel)
     
     consumoMedio <- distancia / combustivel
     
     escreval("Consumo medio = ", consumoMedio:8:3)




Fimalgoritmo